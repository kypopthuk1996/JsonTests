    {
      "Name": "create_room_and_add_user",
      "Description": "Абонент создает конфу используя полного feature-кода. Абонент А добавляет в нее абонента Б.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/ss/activate %%3.Number%% teleconference",
            "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% teleconference",
            "/domain/%%DEV_DOM%%/ss/enable %%0.Number%% chold ctr clip",
            "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% chold",
            "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% ctr",
            "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% clip"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "Name": "RECV_ADD_TO_ROOM",
              "Commands": [
                {
                  "SourceFile": "ss_coral_teleconference/uas_recv_re_invite.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 3,
              "Name": "CREATE_ROOM",
              "Commands": [
                {
                  "SourceFile": "ss_coral_teleconference/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN *470*%%3.Number%%# -set CDPNDOM %%DEV_DOM%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 0,
              "Name": "ADD_USER_TO_ROOM",
              "Commands": [
                {
                  "SourceFile": "ss_coral_teleconference/uac_unattended_succ_transfer.xml",
                  "Options": "-m 1 -set FIRST_CDPN %%1.Number%% -set SECOND_CDPN *472*%%3.Number%%# -set CDPNDOM %%DEV_DOM%% -set CGPNDOM %%DEV_DOM%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "30s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% chold",
            "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% ctr",
            "/domain/%%DEV_DOM%%/ss/deactivate %%0.Number%% clip",
            "/domain/%%DEV_DOM%%/ss/disable %%0.Number%% chold ctr clip"
          ]
        }
